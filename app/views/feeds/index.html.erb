<% @feeds.each do |feed| %>
  <%= feed.url %>
  <%= feed.title %> <br />
  <% feed.items.each do |item| %>
    <%= item.title %> <br />
    <%= item.body.truncate(50) %> <br />
    <%= link_to "もっと見る", item_path(item) %>
    <br />
  <% end %>
  <%= link_to "編集", edit_feed_path(feed) %>
  <%= button_to "削除", feed, method: :delete %>
<% end %>

<%= link_to '新規作成', new_feed_path %>
